    "axios": "^1.7.9",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.2",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.1.6",
    "express-partials": "^0.3.0",
    "mqtt": "^5.3.5",
    "mysql2": "^3.8.0",
    "openapi-typescript": "^6.7.5",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  }
